<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds.  Do not translate. -->

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- PEX Maintainer -->
    <string name="pex_maintainer_name" translatable="false">SMGReborn x Mauls</string>
    <string name="pex_maintainer_group" translatable="false">https://t.me/maulanariaddy</string>

    <!-- Battery Health -->
    <string name="config_batteryRemainingCapacityNode">/sys/class/power_supply/battery/charge_full</string>
    <string name="config_batteryDesignCapacityNode">/sys/class/power_supply/battery/charge_full_design</string>

    <!-- Whether to enable Lineage Fast Charge Preference -->
    <bool name="config_lineageFastChargeSupported">true</bool>
</resources>
